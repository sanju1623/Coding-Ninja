import java.util.*;

public class Solution {

	// input - input array
	public static boolean containsConsecutiveNumbers(int[] input){
		// Write your code here		

      Arrays.sort(input);
      for(int i=0;i+1<input.length;i++)
      {
        if(input[i]+1!=input[i+1])
          return false;
      }
      return true;
	}
	
}
